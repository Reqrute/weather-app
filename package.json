{
  "name": "react-project-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "eslint:test": "eslint src --ext .js,.jsx",
    "eslint:fix": "eslint --fix src --ext .js,.jsx",
    "cypress": "start-server-and-test start http://localhost:3000 cypress:open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:test": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "create-react-app": "^5.0.1",
    "cypress": "^12.8.1",
    "react-google-calendar-api": "^2.2.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "styled-components": "^5.3.9",
    "typescript": "*"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "react-test-renderer": "^18.2.0"
  }
}
