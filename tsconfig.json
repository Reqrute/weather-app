{
    "compilerOptions": {
      "baseUrl": "src",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "strictNullChecks": true ,
      "resolveJsonModule": true,
      "paths": {
        "@components/*": ["components/*"],
        "@store/*": ["store/*"],
        "@constants/*": ["constants/*"],
        "@Common/*": ["components/Common/*"],
        "@pages/*": ["components/pages/*"],
        "@assets/*": ["assets/*"],
        "@services/*": ["services/*"]
      },
    },
    "include": ["**/*"],
    "exclude": ["node_modules"]
  }
  // "include": ["src/**/*"],